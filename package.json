{
  "name": "video-converter",
  "description": "video conversion program",
  "author": "NKWOO",
  "version": "0.1.1",
  "private": true,
  "main": "src/electron-starter.js",
  "homepage": "./",
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "start": "nf start",
    "build-installer-mac": "npm run react-build && electron-builder -m",
    "build-installer-win": "npm run react-build && electron-builder -w",
    "build-installer-linux": "npm run react-build && electron-builder -l",
    "build-portable-win32": "electron-builder --win portable --ia32",
    "build-portable-win64": "electron-builder --win portable --x64"
  },
  "build": {
    "extends": null,
    "directories": {
      "buildResources": "public"
    },
    "target": "zip"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.4",
    "@types/jest": "^26.0.20",
    "@types/node": "^12.20.4",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "foreman": "^3.0.1",
    "is-electron": "^2.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.2",
    "web-vitals": "^1.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^11.3.0",
    "electron-builder": "^22.9.1"
  }
}
